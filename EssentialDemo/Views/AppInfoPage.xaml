<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="EssentialDemo.Views.AppInfoPage"
             Title="App Info">
    <ContentPage.Content>
        <ScrollView Margin="12">
            <StackLayout>
                <Label Text="App Name" />
                <Label x:Name="AppNameLabel" />
                <Label Text="Package Name" />
                <Label x:Name="PackageNameLabel" />
                <Label Text="Version" />
                <Label x:Name="VersionLabel"/>
                <Label Text="Build" />
                <Label x:Name="BuildLabel"/>
            </StackLayout>
        </ScrollView>
    </ContentPage.Content>
</ContentPage>